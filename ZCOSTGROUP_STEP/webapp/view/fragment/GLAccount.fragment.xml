<core:FragmentDefinition
   xmlns="sap.ui.comp.valuehelpdialog"
   xmlns:m="sap.m"
   xmlns:fb="sap.ui.comp.filterbar"
   xmlns:core="sap.ui.core">
   <ValueHelpDialog
      title="{i18n>GLACCOUNT}"
      ok=".onValueHelpOkPress"
      cancel=".onValueHelpCancelPress"
      afterClose=".onValueHelpAfterClose"
      supportRanges="false"
      key="GLAccount"
      descriptionKey="GLAccountName">
      <filterBar>
         <fb:FilterBar advancedMode="true" search=".onFilterBarSearch" isRunningInValueHelpDialog="true">
            <fb:filterGroupItems>
               <fb:FilterGroupItem
                  groupName="__$INTERNAL$"
                  name="GLAccount"
                  label="{i18n>GLACCOUNT}"
                  visibleInFilterBar="true">
                  <fb:control>
                     <m:Input name="GLAccount" />
                  </fb:control>
               </fb:FilterGroupItem>
               <fb:FilterGroupItem
                  groupName="__$INTERNAL$"
                  name="GLAccountName"
                  label="{i18n>GLACCOUNTNAME}"
                  visibleInFilterBar="true">
                  <fb:control>
                     <m:Input name="GLAccountName" />
                  </fb:control>
               </fb:FilterGroupItem>
            </fb:filterGroupItems>
         </fb:FilterBar>
      </filterBar>
   </ValueHelpDialog>
</core:FragmentDefinition>